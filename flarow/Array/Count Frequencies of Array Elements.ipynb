   "source": [
    "# Time Complexity: O(n); Space: O(1)\n",
    "def freq_count(arr):\n",
    "    n = len(arr)\n",
    "    for i in range(n):\n",
    "        arr[i] -= 1\n",
    "    for i in range(n):\n",
    "        arr[arr[i] % n] += n\n",
    "    for i in range(n):\n",
    "        print('Frequency of', i + 1, 'is:', arr[i] // n)\n",
    "\n",
    "freq_count([5, 2, 7, 7, 5, 5, 2])"
   ]
  },

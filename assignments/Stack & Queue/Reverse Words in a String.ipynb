    "def reverse_words(s):\n",
    "    res, stack = '', []\n",
    "    s = s.split()\n",
    "    for w in s:    stack.append(w)\n",
    "    while stack:\n",
    "        res += stack.pop() + ' '\n",
    "    return res[:-1]\n",
    "\n",
    "if __name__=='__main__':\n",
    "    s = \"the sky is blue\"\n",
    "    print(reverse_words(s))"
   ]
